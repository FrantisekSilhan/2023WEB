@model string
@using CookieMaker2023P3A.Services
@{
    List<TempDataExtension.MessageData> messages = TempData.GetMessages("messagetext");
}

@if (messages != default)
{
    <div class="message-box">
        @foreach (var message in messages)
        {
            <div class="alert alert-@message.MessageType" role="alert">
                @message.MessageText
            </div>
        }
    </div>
}